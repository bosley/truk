fn main() : i32 {
  var m: map[i32] = make(@map[i32]);
  
  m["first"] = 10;
  m["second"] = 5;
  m["third"] = 15;
  
  var ptr1: *i32 = m["first"];
  var ptr2: *i32 = m["second"];
  var ptr3: *i32 = m["third"];
  
  var result: i32 = 0;
  if ptr1 != nil {
    result = result + *ptr1;
  }
  if ptr2 != nil {
    result = result + *ptr2;
  }
  if ptr3 != nil {
    result = result + *ptr3;
  }
  
  delete(m);
  return result;
}
