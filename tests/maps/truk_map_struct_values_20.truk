struct Point {
  x: i32,
  y: i32
}

fn main() : i32 {
  var m: map[Point] = make(@map[Point]);
  
  var p: Point = Point{x: 10, y: 10};
  m["origin"] = p;
  
  var ptr: *Point = m["origin"];
  if ptr != nil {
    var result: i32 = (*ptr).x + (*ptr).y;
    delete(m);
    return result;
  }
  
  delete(m);
  return 0;
}
