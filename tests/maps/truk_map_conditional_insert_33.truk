fn main() : i32 {
  var m: map[*u8, i32] = make(@map[*u8, i32]);
  
  var condition: bool = true;
  
  if condition {
    m["yes"] = 33;
  } else {
    m["no"] = 0;
  }
  
  var ptr1: *i32 = m["yes"];
  var ptr2: *i32 = m["no"];
  
  var result: i32 = 0;
  if ptr1 != nil {
    result = result + *ptr1;
  }
  if ptr2 != nil {
    result = result + *ptr2;
  }
  
  delete(m);
  return result;
}
