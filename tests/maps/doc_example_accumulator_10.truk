fn main() : i32 {
  var m: map[*u8, i32] = make(@map[*u8, i32]);
  
  m["total"] = 0;
  
  var ptr: *i32 = m["total"];
  if ptr != nil {
    *ptr = *ptr + 10;
  }
  
  ptr = m["total"];
  if ptr != nil {
    var result: i32 = *ptr;
    delete(m);
    return result;
  }
  
  delete(m);
  return 0;
}
