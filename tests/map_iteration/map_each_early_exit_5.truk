fn main() : i32 {
  var m: map[i32] = make(@map[i32]);
  m["a"] = 1;
  m["b"] = 2;
  m["c"] = 3;
  
  var count: i32 = 0;
  each(m, &count, fn(key: *u8, val: *i32, ctx: *i32) : bool {
    *ctx = *ctx + 1;
    if *ctx >= 2 {
      return false;
    }
    return true;
  });
  
  delete(m);
  return count + 3;
}
