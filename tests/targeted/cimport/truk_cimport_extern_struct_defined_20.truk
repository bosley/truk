cimport <time.h>;

extern struct tm {
    tm_sec: i32,
    tm_min: i32,
    tm_hour: i32,
    tm_mday: i32,
    tm_mon: i32,
    tm_year: i32,
    tm_wday: i32,
    tm_yday: i32,
    tm_isdst: i32
}

extern fn time(timer: *i64): i64;
extern fn localtime(timer: *i64): *tm;

fn main(): i32 {
    var t: i64 = time(nil);
    var timeinfo: *tm = localtime(&t);
    return 20;
}
