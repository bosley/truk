fn main() : i32 {
  var arr: [3]**i32;
  
  var v1: *i32 = alloc(@i32);
  *v1 = 5;
  var v2: *i32 = alloc(@i32);
  *v2 = 7;
  var v3: *i32 = alloc(@i32);
  *v3 = 3;
  
  arr[0] = alloc(@*i32);
  *arr[0] = v1;
  arr[1] = alloc(@*i32);
  *arr[1] = v2;
  arr[2] = alloc(@*i32);
  *arr[2] = v3;
  
  var result: i32 = **arr[0] + **arr[1] + **arr[2];
  
  free(arr[0]);
  free(arr[1]);
  free(arr[2]);
  free(v1);
  free(v2);
  free(v3);
  
  return result;
}
