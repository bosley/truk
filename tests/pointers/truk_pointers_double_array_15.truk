fn main() : i32 {
  var arr: [3]**i32;
  
  var v1: *i32 = make(@i32);
  *v1 = 5;
  var v2: *i32 = make(@i32);
  *v2 = 7;
  var v3: *i32 = make(@i32);
  *v3 = 3;
  
  arr[0] = make(@*i32);
  *arr[0] = v1;
  arr[1] = make(@*i32);
  *arr[1] = v2;
  arr[2] = make(@*i32);
  *arr[2] = v3;
  
  var result: i32 = **arr[0] + **arr[1] + **arr[2];
  
  delete(arr[0]);
  delete(arr[1]);
  delete(arr[2]);
  delete(v1);
  delete(v2);
  delete(v3);
  
  return result;
}
