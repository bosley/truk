enum Score : i32 {
    ZERO = 0,
    FIVE = 5,
    TEN = 10,
    FIFTEEN = 15
}

fn main() : i32 {
    var scores: [3]Score = [Score.FIVE, Score.TEN, Score.FIFTEEN];
    var sum: i32 = 0;
    var i: i32 = 0;
    while i < 3 {
        sum = sum + (scores[i] as i32);
        i = i + 1;
    }
    return sum;
}
