enum Status : i32 {
    INACTIVE = 0,
    ACTIVE = 99,
    SUSPENDED = 200
}

fn main() : i32 {
    var m: map[i32, i32] = make(@map[i32, i32]);
    
    m[1] = Status.INACTIVE as i32;
    m[2] = Status.ACTIVE as i32;
    m[3] = Status.SUSPENDED as i32;
    
    var ptr: *i32 = m[2];
    var result: i32 = 0;
    if ptr != nil {
        result = *ptr;
    }
    
    delete(m);
    return result;
}
