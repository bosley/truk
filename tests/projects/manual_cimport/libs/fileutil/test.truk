import "fileutil/lib_impl.truk";

fn main(): i32 {
    var size: i64 = get_file_size("test_data/input.txt");
    if (size < 0) {
        return 1;
    }
    
    var first: i32 = get_first_byte("test_data/input.txt");
    if (first < 0) {
        return 1;
    }
    
    var num: i32 = get_number_from_file("test_data/input.txt");
    if (num != 42) {
        return 1;
    }
    
    return 0;
}
